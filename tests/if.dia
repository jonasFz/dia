external function print(var a u32)->void;
external function assert(var a u32)->void;

function test_if(var a u32) -> u32 {
	if a == 1 {
		return 1;
	}else if a == 2 {
		return 2;
	}else if a == 3 {
		return 3;
	}else {
		return 0;
	}

	return -2;
}

function main() -> void {

	assert(test_if(1) == 1);
	assert(test_if(2) == 2);
	assert(test_if(3) == 3);
	assert(test_if(4) == 0);

	return;
}
